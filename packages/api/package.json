{
  "name": "@livepeer/api",
  "description": "Livepeer API node for controlling streams",
  "version": "1.0.0-alpha.5",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "src/index.js",
  "bin": {
    "@livepeer/api": "dist/cli.js"
  },
  "scripts": {
    "start": "node-dev -r esm src/cli.js",
    "prepare": "babel --out-dir=dist --source-maps=true src",
    "build": "npm run prepare",
    "test": "jest --env node --no-cache src",
    "postgres": "docker run --rm --name postgres -p 5432:5432 -v \"$(pwd)/data/postgres:/var/lib/postgresql/data\" postgres"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "level": "^5.0.1",
    "morgan": "^1.9.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "esm": "^3.2.22",
    "jest": "^23.6.0",
    "node-dev": "^3.1.3"
  },
  "homepage": "https://livepeer.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeerjs.git"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "browserslist": [
    "node 10"
  ]
}
